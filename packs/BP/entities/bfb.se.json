{
	"format_version": "1.21.30",
	"minecraft:entity": {
		"description": {
			"identifier": "sc:bfb",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			"destroy": {
				"minecraft:instant_despawn": {}
			}
		},
		"components": {
			"minecraft:projectile": {
				"on_hit": {
