				"time_down_event": {
					"event": "destroy_event"
				}
			}
		},
		"events": {
			"destroy_event": {
				"add": {
					"component_groups": [
						"destroy"
					]
				}
			},
			"particles": {
				"queue_command": {
					"command": [
						"particle sc:bfg_particle ~ ~ ~",
