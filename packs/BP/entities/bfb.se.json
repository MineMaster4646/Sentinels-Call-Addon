						"damage": 30
					},
					"stick_in_ground": {
						"shake_time": 0
					},
					"definition_event": {
						"affect_projectile": true,
						"event_trigger": {
							"event": "destroy_event",
							"filters": [
								{
									"test": "has_component",
									"value": "minecraft:projectile",
									"operator": "!=",
									"subject": "other"
								}
							]
						}
					},
					"remove_on_hit": {}
				},
				"power": 1,
				"uncertainty_base": 5,
				"offset": [
					5,
					0,
					10
				],
				"anchor": 1,
				"should_bounce": false,
				"destroy_on_hurt": true,
				"gravity": 0
			},
			"minecraft:type_family": {
				"family": [
					"bullet"
				]
			},
			"minecraft:collision_box": {
				"width": 0.1,
				"height": 0.1
			},
			"minecraft:health": {
				"value": 1
			},
			"minecraft:damage_sensor": {
				"triggers": {
					"deals_damage": false
				}
			},
			"minecraft:environment_sensor": {
				"triggers": [
					{
						"filters": [
							{
								"test": "on_ground",
								"subject": "self"
							}
